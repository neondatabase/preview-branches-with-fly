{
  "name": "node-prisma-neon-api",
  "version": "1.0.0",
  "description": "A Node.js API built using Prisma ORM and Neon",
  "main": "dist/index.js",
  "scripts": {
    "dev": "pnpm tsx ./src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint ./src --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "build": "pnpm tsc --build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "neon",
    "prisma",
    "node",
    "typescript",
    "api"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.1.1",
    "prisma": "^5.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}